<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Page">
    <children>
        <Label fx:id="label" layoutX="25.0" layoutY="150.0" prefHeight="100.0" prefWidth="300.0"
               style="-fx-alignment: CENTER; -fx-wrap-text: true;">
            <font>
                <Font name="Arial" size="13.0"/>
            </font>
        </Label>
        <ButtonBar prefHeight="40.0" prefWidth="350.0">
            <buttons>
                <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logOut" text="Logout">
                    <font>
                        <Font name="Arial" size="13.0"/>
                    </font>
                </Button>
            </buttons>
            <padding>
                <Insets right="6.0"/>
            </padding>
        </ButtonBar>
    </children>
</AnchorPane>
